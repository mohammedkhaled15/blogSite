<section class="article">
  <h2 class="articles__heading">Searched Posts</h2>
  <ul class="article-ul">
    <% result.forEach(post => { %>
    <li>
      <a href="/post/<%= post._id %>">
        <span><%= post.title %></span>
        <span class="article-list__date"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>
    <% }) %>
  </ul>
</section>
